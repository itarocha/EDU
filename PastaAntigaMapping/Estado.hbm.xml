<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Dardani.EDU.Entities"
                   namespace="Dardani.EDU.Entities.Model">
  <class name="Estado" table="EDU_ESTADO">
    <id name="Id" column="ID_ESTADO">
      <generator class="native"/>
    </id>
    <property name="Descricao" not-null="true" column="DS_ESTADO" length="64"/>
    <property name="Sigla" not-null="true" column="DS_SIGLA" length="2"/>
    <property name="ValorEducacenso" column="VL_EDUCACENSO"  not-null="true"/>
    <property name="FlagAtivo" not-null="true" column="FL_ATIVO" length="1"/>
  </class>
</hibernate-mapping>
