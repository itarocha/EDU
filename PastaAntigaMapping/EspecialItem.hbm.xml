<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Dardani.EDU.Entities"
                   namespace="Dardani.EDU.Entities.Model">
  <class name="EspecialItem" table="EDU_ESPECIAL_ITEM">
    <id name="Id" column="ID_ESPECIAL_ITEM">
      <generator class="native"/>
    </id>
    <property name="Descricao" not-null="true" column="DS_ESPECIAL_ITEM" length="64"/>
    <many-to-one name="EspecialCategoria" class="EspecialCategoria"  column="ID_ESPECIAL_CATEGORIA" not-null="true" />
    <property name="ValorEducacenso" column="VL_EDUCACENSO"  not-null="true"/>
    <property name="FlagPadrao" not-null="true" column="FL_PADRAO" length="1"/>
    <property name="FlagAtivo" not-null="true" column="FL_ATIVO" length="1"/>
  </class>
</hibernate-mapping>
