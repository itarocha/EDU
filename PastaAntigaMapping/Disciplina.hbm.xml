<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Dardani.EDU.Entities"
                   namespace="Dardani.EDU.Entities.Model">
  <class name="Disciplina" table="EDU_DISCIPLINA">
    <id name="Id" column="ID_DISCIPLINA">
      <generator class="native"/>
    </id>
    <property name="Descricao" not-null="true" column="DS_DISCIPLINA" length="128"/>
    <property name="DescricaoAbreviada" not-null="true" column="DS_ABREVIATURA" length="8"/>
    <property name="ValorEducacenso" column="VL_EDUCACENSO"  not-null="true"/>
    <property name="FlagAtivo" column="FL_ATIVO" length="1" not-null="true"/>
  </class>
</hibernate-mapping>
