<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Dardani.EDU.Entities"
                   namespace="Dardani.EDU.Entities.Model">
  <class name="Pais" table="EDU_PAIS">
    <id name="Id" column="ID_PAIS">
      <generator class="native"/>
    </id>
    <property name="Descricao" not-null="true" column="DS_PAIS" length="64"/>
    <property name="Codigo" not-null="true" column="VL_CODIGO" />
    <property name="FlagPadrao" not-null="true" column="FL_PADRAO" length="1"/>
    <property name="FlagAtivo" not-null="true" column="FL_ATIVO" length="1"/>
  </class>
</hibernate-mapping>
