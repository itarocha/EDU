@model Dardani.EDU.Entities.Model.PeriodoAula

@{
    ViewBag.Title = ViewBag.Acao;
}

<script type="text/javascript">
    $(function() {
        $("#HoraInicio").focus();
    });
</script>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(false)

    <div class="row">

        <div class="col-md-6">
            <label class="input-label f-left">@Html.LabelFor(model => model.HoraInicio):</label>
            @Html.TextBoxFor(model => model.HoraInicio, new { @class = "input-text f-left", @alt="time" })
        </div>
        <div class="col-md-6">
            <label class="input-label f-left">@Html.LabelFor(model => model.HoraTermino):</label>
            @Html.TextBoxFor(model => model.HoraTermino, new { @class = "input-text f-left", @alt="time"})
        </div>
    </div>
    @Html.HiddenFor(model => model.Id)
    <div class="row">
        <div class="col-md-12 pt-20">
            <input type="submit" value="Gravar" class="btn btn-success" />
            @Html.ActionLink("Voltar", "Index", "PeriodosAulas", null, new { @class = "btn btn-default" })
        </div>
    </div>
}
